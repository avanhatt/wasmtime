        (=>
            (and (= alu_op (ALUOp.AndNot)) (= size (OperandSize.Size64)))
            (and
                (= rn v544)
                (= rm v546)
                (= v551 (bvand (extract 63 0 v544) (bvnot (extract 63 0 v546))))
                (= rd v551)))
        (=>
            (and (= alu_op (ALUOp.Eor)) (= size (OperandSize.Size64)))
