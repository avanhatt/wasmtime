(spec
    (MInst.AluRRR alu_op size rd rn rm)
    (provide
        (=>
            (and (= alu_op (ALUOp.Add)) (= size (OperandSize.Size64)))
            (and
                (= rn v536)
                (= rm v538)
                (=
                    v581
                    (bvadd
